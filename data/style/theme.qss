    border-radius: 4px;
}


/*************************ContainerList**********************/
QPushButton#btnCreate
{
    background-color:#2EB3FF;
    border:none ;
    border-radius: 4px;
    font-size:14px;
    color:#ffffff;
}
QPushButton#btnCreate:hover
{
    background-color:#77ceff;
}
QPushButton#btnCreate:focus
{
    outline: none;
}

/* the subcontrols below are used only in the MenuButtonPopup mode */
