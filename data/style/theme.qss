/***********************MainWindow************************/
QLabel
{
    font-size:14px;
}

MainWindow QWidget#widget_content
{
    background:#2d2d2d;
    border-radius:4px;
}
MainWindow #listWidget
{
    background:#2d2d2d;
    border:none;
    border-radius:4px;
    outline:none;
}
MainWindow #listWidget::item
{
    background:transparent;
    border:none;
    border-radius:4px;
}
MainWindow #listWidget::Item:selected
{
    background-color:#393939;
}
MainWindow #listWidget::Item:hover
{
    background-color:#393939;
}

MainWindow GuideItem
{
    background-color:transparent;
}
MainWindow GuideItem #label_text
{
    color:#ffffff;
    font-size:14px;
}

MainWindow QLabel#lab_user_icon
{
    image: url(:/images/user.png);
}

MainWindow QLabel#lab_dot
{
    image: url(:/images/dot.svg);
}

MainWindow > BubbleTipButton
{
    border-radius:4px;
}

QPushButton#btn_approval,
QPushButton#btn_transmission,
QPushButton#btn_warning,
QPushButton#btn_user
{
    background-color:transparent;
    border:none;
    border-radius:0px;
}
QPushButton#btn_approval:focus,
QPushButton#btn_transmission:focus,
QPushButton#btn_warning:focus,
QPushButton#btn_user:focus
{
    outline:none;
}

QPushButton#btn_user::menu-indicator
{
    image: url(:/images/down-arrow.svg);
    subcontrol-position: right center;
    subcontrol-origin: padding;
/*    width:16px;
    height:16px*/
/*    left: -5px;*/
}

/*************************TablePage*************************/
QToolButton#btn_refresh
{
    border:none;
}
TablePage QTableView{
/*    border: 1px solid  red;		/*表格外边框*/
    border:none;
    border-top:1px solid #393939;
    padding: 0px;		/*内边距*/
    margin: 0px;			/*外边距*/
    color: #ffffff;		/*表格文本的颜色*/
    background: #2d2d2d;	/*表格背景色*/
    gridline-color:	#2d2d2d;		/*网格边框*/
    font-size: 14px;					/*表格文本大小*/
/*    font-family: "PingFang SC Regular";		/*表格文本字体*/
}
TablePage QTableView::item
{
    padding-left:10px;
    border-bottom : 1px solid #393939;
/*     border-left : 1px solid #393939;*/
    height:60px;
}
TablePage QTableView::indicator
{
    width:14px;
    height:14px;
}
TablePage QTableView::indicator:unchecked
{
    image: url(:/images/check-default.svg);
}
TablePage QTableView#tableView::indicator:checked
{
    image: url(:/images/checked.svg);
}
TablePage #lineEdit_search
{
    border:1px solid #393939 ;
    background:#222222;
    color:#919191;
    border-radius: 4px;
    font-size:14px;
}
TablePage #lineEdit_search:focus
{
    border:1px solid #409EFF ;
}
TablePage #label_search_tips
{
    color:#F57900;
}
TablePage
{
    background:transparent;

}

