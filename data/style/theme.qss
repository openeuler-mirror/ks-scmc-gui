/***********************ContainerSetting******************/
ContainerSetting #btn_confirm,#m_pBtnSave
{
    background-color:#409EFF;
    border: 1px solid #409EFF;
    border-radius: 6px;
    color:#ffffff;
    font-size:14px;
}
ContainerSetting #btn_confirm:hover,#m_pBtnSave:hover
{
    background-color:#66B1FF;
}
