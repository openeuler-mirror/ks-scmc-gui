}

ContainerSetting
{
    background-color:#ffffff;
}

ContainerSetting QLabel
{
    font-size:14px;
    color:#000000;
}

ContainerSetting QComboBox
{
    border: 1px solid #E4E7ED;
    border-radius: 6px;
