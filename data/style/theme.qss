/***********************ContainerSetting******************/
ContainerSetting #btn_confirm,#m_pBtnSave
{
    background-color:#409EFF;
    border: 1px solid #409EFF;
    border-radius: 6px;
    color:#ffffff;
    font-size:14px;
}
ContainerSetting #btn_confirm:hover,#m_pBtnSave:hover
{
    background-color:#66B1FF;
}
ContainerSetting #btn_cancel
{

    background-color:#ffffff;
    border: 1px solid #E4E7ED;
    border-radius: 6px;
    color:#000000;
    font-size:14px;
}
ContainerSetting #btn_cancel:hover
{
    background-color:#EBEEF5
}
ContainerSetting #btn_cancel:focus,ContainerSetting #btn_confirm:focus
{
    outline:none;
}

ContainerSetting
{
    background-color:#ffffff;
}

ContainerSetting QLabel
{
    font-size:14px;
    color:#000000;
}

ContainerSetting QComboBox
{
    border: 1px solid #E4E7ED;
    border-radius: 6px;
    padding-left:10px;
    color:#000000;
}
ContainerSetting QComboBox:drop-down
{
    border:none;
    background-color:transparent;
    padding-right:10px;
}
ContainerSetting QComboBox::down-arrow
{
    image: url(:/images/down-arrow.svg);
}

ContainerSetting QComboBox QAbstractItemView
{
    background:#ffffff;
    color:#303133;
    outline:none;
    border: 1px solid #E4E7ED;

}
ContainerSetting QComboBox QAbstractItemView::item
{
    height:30px;
}

ContainerSetting QComboBox QAbstractItemView::item:selected
{
   background-color:#EDF3FF;
   color:#303133;
}

ContainerSetting QLineEdit
{
    border:1px solid #E4E7ED;
    border-radius:6px;
    color:#000000;
}
ContainerSetting QLineEdit:focus
{
    border:1px solid #409EFF;
}
ContainerSetting GuideItem
{
    border:none;
}
ContainerSetting QListWidget
{
    outline:none;
}
ContainerSetting QListWidget::item:hover
{
    background:#EBEEF5;
}
ContainerSetting QListWidget::item:selected
{
    background:#EBEEF5;

}
ContainerSetting QListWidget::item:focus
{
    outline:none;
}

ContainerSetting GuideItem #delBtn
{
    border:none;
    background-color:transparent;
/*    outline: none;*/
}
ContainerSetting #btn_add
{
    background-color:#0099FF;
    color:#ffffff;
    border:none;
    border-radius:6px;
}
ContainerSetting #btn_add:hover
{
    background-color:#66B1FF;
}
ContainerSetting #btn_add:pressed
{
    background-color:#0099FF;
}

ContainerSetting QTabWidget::pane {
    border:none;
}

ContainerSetting QTabWidget::tab-bar {
   /*  left: 5px; move to the right by 5px */
   min-height:40px;
   background: #E5EBF6;
   alignment: left;
}

/* Style the tab using the tab sub-control. Note that
    it reads QTabBar _not_ QTabWidget */
ContainerSetting QTabBar::tab {
    background: #E5EBF6;
    border:none;
/*    min-width: 8ex;*/
    padding: 10px;
    color:#000000;
}

ContainerSetting QTabBar::tab:selected {
    color:#43A3F5;
    border-bottom:2px solid #43A3F5;
    outline:none;
}

/****************************Graphic*************************/
QCheckBox#graphic_checkBox::indicator {
    width: 30px;
    height: 30px;
}

QCheckBox#graphic_checkBox::indicator:unchecked
{
    image: url(:/images/close.svg);
}
QCheckBox#graphic_checkBox::indicator:checked
{
    image: url(:/images/open.svg);
}


